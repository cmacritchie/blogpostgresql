{
  "name": "postgresql",
  "version": "1.0.0",
  "description": "run postgresql  brew services start postgresql",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "pre-production": "SET NODE_ENV=preproduction&& nodemon src/index.js",
    "server": "SET NODE_ENV=dev&& nodemon src/index.js",
    "client": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install --prefix ../client && npm run build --prefix ../client",
    "test": "NODE_ENV=test jest",
    "windows:test": "SET NODE_ENV=test&& jest",
    "test:watch": "SET NODE_ENV=dev&& jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sharp": "^0.27.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  },
  "proxy": "http://localhost:5000"
}
